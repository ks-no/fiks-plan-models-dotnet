{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://no.ks.fiks.gi.plan.innsyn/Dispensasjoner.v2.schema.json",
    "title": "Dispensasjoner",
    "definitions": {
        "dispensasjon": {
            "identifikasjon": {
                "type": "string"
            },
            "dispensasjon": {
                "type": "object",
                "properties": {
                    "kodeverdi": {
                        "type": "string"
                    },
                    "kodebeskrivelse": {
                        "type": "string"
                    }
                }
            },
            "nasjonalArealplanId": {
                "type": "object",
                "properties": {
                    "administrativEnhet": {
                        "properties": {
                            "kommunenummer": {
                                "description": "Kommunenummer",
                                "type": "string"
                            },
                            "fylkesnummer": {
                                "description": "Fylkesnummer",
                                "type": "string"
                            },
                            "landskode": {
                                "description": "Landskode",
                                "type": "string"
                            }
                        },
                        "oneOf": [
                            { "required": [ "kommunenummer" ] },
                            { "required": [ "fylkesnummer" ] },
                            { "required": [ "landskode" ] }
                        ]
                    },
                    "planidentifikasjon": {
                        "type": "string"
                    }
                },
                "required": [
                    "administrativEnhet",
                    "planidentifikasjon"
                ]
            },
            "begrunnelse": {
                "type": "string"
            },
            "vedtaksdato": {
                "type": "string",
                "format": "date"
            },
            "posisjon": {
                "description": "Punkt",
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "enum": [
                            "Point"
                        ]
                    },
                    "coordinates": {
                        "type": "array",
                        "minItems": 2,
                        "items": {
                            "type": "number"
                        }
                    },
                    "crs": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "string",
                                "enum": [
                                    "name"
                                ]
                            },
                            "properties": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "required": [
                    "type",
                    "coordinates"
                ]
            },
            "vertikalnivaa": {
                "type": "object",
                "properties": {
                    "kodeverdi": {
                        "type": "string"
                    },
                    "kodebeskrivelse": {
                        "type": "string"
                    }
                }
            },
            "beskrivelse": {
                "type": "string"
            },
            "innvilgetDispensasjon": {
                "type": "boolean"
            },
            "saksnummer": {
                "type": "object",
                "properties": {
                    "saksaar": {
                        "type": "integer"
                    },
                    "sakssekvensnummer": {
                        "type": "integer"
                    }
                },
                "required": [
                    "saksaar",
                    "sakssekvensnummer"
                ]
            },
            "varigDispensasjon": {
                "type": "boolean"
            },
            "varighetFra": {
                "type": "string",
                "format": "date"
            },
            "varighetTil": {
                "type": "string",
                "format": "date"
            }
        }
    },
    "type": "object",
    "properties": {
        "dispensasjoner": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/dispensasjon"
            }
        }
    }
}